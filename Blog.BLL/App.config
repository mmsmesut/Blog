<?xml version="1.0" encoding="utf-8" ?>
<configuration> 
  <connectionStrings>
     <add  name="BlogDBContext"
         connectionString="data source=.;initial catalog=BLOG;integrated security=True;MultipleActiveResultSets=True;App=EntityFramework;"
         providerName="System.Data.Sql" 
    />  
  </connectionStrings>

  <appSettings>
    <add key="Context" value="BlogDBContext" />
  </appSettings>
</configuration>